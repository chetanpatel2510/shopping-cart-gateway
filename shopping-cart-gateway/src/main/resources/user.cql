CREATE KEYSPACE IF NOT EXISTS USER_KEY_SPACE WITH replication = {'class':'SimpleStrategy', 'replication_factor':1};

CREATE TABLE USER_KEY_SPACE.USERS (
	USERNAME TEXT,
	PASSWORD TEXT,
	ACCOUNT_NOT_LOCKED BOOLEAN,
	ENABLED BOOLEAN,
	ACCOUNT_NOT_EXPIRED BOOLEAN,
	CREDENTIAL_NOT_EXPIRED BOOLEAN,
	ROLES SET<TEXT>,
	CREATED_BY TEXT,
	CREATED_ON TIMESTAMP,
	LAST_UPDATED_BY TEXT,
	LAST_UPDATED_ON TIMESTAMP,
	PRIMARY KEY (USERNAME)
);
